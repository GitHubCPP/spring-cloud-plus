<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <!-- 父依赖-->
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.3.2.RELEASE</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>org.gourd.hu</groupId>
    <artifactId>spring-cloud-plus</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <modules>
        <module>base-plus-starter</module>
        <module>core-plus-starter</module>
        <module>mq-plus-starter</module>
        <module>activiti-plus-starter</module>
        <module>cloud-plus-starter</module>
        <module>cache-plus-starter</module>
        <module>es-plus-starter</module>
        <module>document-plus-starter</module>
        <module>log-plus-starter</module>
        <module>quartz-plus-starter</module>
        <module>notice-plus-starter</module>
        <module>rbac-plus-starter</module>
        <module>flyway-plus-starter</module>
        <module>generator-boot</module>
        <module>boot-admin-web</module>
        <module>mongo-plus-starter</module>
        <module>gateway-web</module>
        <module>admin-web</module>
        <module>sub-web</module>
        <module>sub-api</module>
        <module>openapi-web</module>
    </modules>
    <!--版本号统一管理-->
    <properties>
        <!--此版本修复了Apache-Tomcat-Ajp漏洞-->
        <tomcat.version>9.0.31</tomcat.version>
        <cloud.version>1.0-SNAPSHOT</cloud.version>
        <slf4j-api.version>1.7.25</slf4j-api.version>
        <mysql.version>8.0.21</mysql.version>
        <fastjson.version>1.2.58</fastjson.version>
        <!--mybatis-plus-->
        <mybatis-plus.version>3.4.0</mybatis-plus.version>
        <dynamic-plus.version>3.2.1</dynamic-plus.version>
        <knife4j.version>2.0.0</knife4j.version>
        <spring-cloud.version>Hoxton.SR8</spring-cloud.version>
        <alibaba-cloud.version>2.2.3.RELEASE</alibaba-cloud.version>
        <netty-websocket.version>0.9.5</netty-websocket.version>
        <redisson.version>3.10.6</redisson.version>
        <fast-dfs.version>1.27.2</fast-dfs.version>
        <artofsolving.jodconverter>2.2.1</artofsolving.jodconverter>
        <openoffice.version>1.0.3.RELEASE</openoffice.version>
        <boot-admin.version>2.2.0</boot-admin.version>
        <jetcache.version>2.5.9</jetcache.version>
        <retry.version>1.2.5.RELEASE</retry.version>
        <shiro.version>1.4.0</shiro.version>
        <jwt.version>3.3.0</jwt.version>
        <activiti.version>6.0.0</activiti.version>
        <p6spy.version>1.6.2</p6spy.version>
        <easyexcel.version>2.1.4</easyexcel.version>
        <openoffice.version>1.0.3.RELEASE</openoffice.version>
        <jasypt.version>1.16</jasypt.version>
        <kaptcha.version>1.1.0</kaptcha.version>
        <pinyin.version>2.5.1</pinyin.version>
        <flying-saucer.version>9.1.9</flying-saucer.version>
        <itext.version>2.1.7</itext.version>
        <emoji-java.version>5.1.1</emoji-java.version>
        <hdrhistogram.version>2.1.1</hdrhistogram.version>
        <hutool.version>5.2.1</hutool.version>
        <image.version>2.0.1</image.version>
        <csv.version>1.8</csv.version>
        <minio.version>8.0.1</minio.version>
        <jsch.version>0.1.55</jsch.version>
    </properties>
    <!--依赖统一管理-->
    <dependencyManagement>
        <dependencies>
            <!--数据库-->
            <dependency>
                <groupId>mysql</groupId>
                <artifactId>mysql-connector-java</artifactId>
                <scope>runtime</scope>
                <version>${mysql.version}</version>
            </dependency>
            <!--spring cloud -->
            <dependency>
                <groupId>org.springframework.cloud</groupId>
                <artifactId>spring-cloud-dependencies</artifactId>
                <version>${spring-cloud.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <!--spring cloud -->
            <dependency>
                <groupId>com.alibaba.cloud</groupId>
                <artifactId>spring-cloud-alibaba-dependencies</artifactId>
                <version>${alibaba-cloud.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <!--fastjson-->
            <dependency>
                <artifactId>fastjson</artifactId>
                <groupId>com.alibaba</groupId>
                <version>${fastjson.version}</version>
            </dependency>
            <dependency>
                <artifactId>guava</artifactId>
                <groupId>com.google.guava</groupId>
                <version>20.0</version>
            </dependency>
            <!--itext-->
            <dependency>
                <groupId>com.lowagie</groupId>
                <artifactId>itext</artifactId>
                <version>${itext.version}</version>
            </dependency>
            <!--gourd-->
            <dependency>
                <groupId>org.gourd.hu</groupId>
                <artifactId>base-plus-starter</artifactId>
                <version>${cloud.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
